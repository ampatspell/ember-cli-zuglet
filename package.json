{
  "name": "zuglet",
  "version": "2.3.2",
  "description": "zuglet next",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "coverage": "COVERAGE=true ember test && open ./coverage/index.html",
    "tags": "git push --follow-tags",
    "bump": "npm version minor; npm run tags",
    "patch": "npm version patch; npm run tags"
  },
  "dependencies": {
    "ember-auto-import": "^1.7.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1",
    "firebase": "^8.0.1",
    "node-fetch": "^2.6.1",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.22.0",
    "ember-cli-code-coverage": "^1.0.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-remark-static": "git+ssh://git@github.com:ampatspell/ember-cli-remark-static-glimmer",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.2",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.22.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.13.0",
    "eslint-plugin-ember": "^9.5.0",
    "eslint-plugin-node": "^11.1.0",
    "fastboot": "^3.1.2",
    "loader.js": "^4.7.0",
    "macro-decorators": "^0.1.2",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.5.0",
    "sass": "^1.29.0",
    "tracked-toolbox": "^1.2.1"
  },
  "fastbootDependencies": [
    "node-fetch",
    "xmlhttprequest",
    "buffer"
  ],
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
