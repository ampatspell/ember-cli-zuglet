{{#ui-block/rows as |rows|}}

  {{#rows.header}}
    {{ui-input/file update=(action 'upload')}}
  {{/rows.header}}

  {{#if ref}}

    {{#rows.row}}
      {{ui-block/json ref.serialized}}
    {{/rows.row}}

    {{#rows.row}}
      {{ui-block/json ref.metadata.serialized}}
    {{/rows.row}}

    {{#rows.row}}
      {{ui-block/json task.serialized}}
    {{/rows.row}}

    {{#if ref.url}}
      {{#rows.row}}
        <img src={{ref.url}}>
      {{/rows.row}}
    {{/if}}

  {{/if}}

{{/ui-block/rows}}
