<div class="route-dev">

  <div class="row">
    isDirty = {{this.isDirty}}
  </div>

  <div class="row">
    <div class="label">Data</div>
    <Json @value={{this.data}}/>
  </div>

  <div class="row">
    <div class="label">Raw</div>
    <Json @value={{this.raw}}/>
  </div>

  <div class="row">
    <Input @type="text" @value={{this.name}} @placeholder="name"/>
  </div>

</div>