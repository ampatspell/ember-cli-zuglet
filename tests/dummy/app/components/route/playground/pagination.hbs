<div class="route-playground-pagination">

  <div class="row">
    <input type="button" value="Insert posts" {{on "click" this.insert}} disabled={{this.isInserting}}/>
    {{#if this.isInserting}}
      Insertingâ€¦
    {{/if}}
  </div>

  <div class="row">
    <input type="button" value="Load more" {{on "click" this.loadMore}}/>
  </div>

  <div class="row">
    {{#each this.queries as |query|}}
      <div class="row">{{query}}</div>
      {{#each query.content as |doc|}}
        <div class="row">{{doc.data.title}}</div>
      {{/each}}
    {{/each}}
  </div>

</div>