<div class="route-index">

  <div class="row">
    <input type="button" value="Toggle" {{on "click" this.toggle}}/>
  </div>

  <div class="row">
    <Json @value={{this.doc}}/>
  </div>

  <div class="row">
    <Json @value={{this.array}}/>
  </div>

  <div class="row">
    {{#each this.array as |doc|}}
      <div>{{doc.data.name}} {{if doc.isActivated 'active' 'inactive'}}</div>
    {{/each}}
  </div>

</div>